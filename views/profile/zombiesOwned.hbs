<div class="catalog">
{{#each user.zombiesOwned}}
<a href="/ownedZombieDetail?zombie_id={{_id}}"
      <div class="ownedZombie shopZombie">
        <div class="imageWrap">
            <img src="{{origin.image}}">
        </div>
        <h3>{{origin.type}}</h3>
        <ul>
        {{#each origin.description}}
          <li>{{this}}</li>
        {{/each}}
        </ul>
      </div>
{{/each}}
</div>